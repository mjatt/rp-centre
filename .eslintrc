{
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "vars-on-top": 0,
    "consistent-return": 0,
    "react/wrap-multilines": 0,
    "react/jsx-wrap-multilines": 0
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "describe": true,
    "it": true,
    "Promise": true
  },
  "env": {
    "node": true
  },
  "extends": "eslint-config-airbnb-es5"
}
